{
  "name": "WEConnect",
  "version": "1.0.0",
  "description": "WEConnect connects businesses to consumers",
  "main": "server/index.js",
  "author": "John David",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/holuborhee/WEConnect"
  },
  "bugs": {
    "url": "https://github.com/holuborhee/WEConnect/issues"
  },
  "homepage": "https://github.com/holuborhee/WEConnect#readme",
  "dependencies": {
    "bcrypt": "^1.0.3",
    "body-parser": "^1.15.1",
    "chai": "^3.5.0",
    "chai-http": "^2.0.1",
    "config": "^1.20.1",
    "express": "^4.13.4",
    "faker": "^4.1.0",
    "morgan": "^1.7.0",
    "pg": "^7.4.1",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.37.4"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "coveralls": "^3.0.0",
    "eslint": "^4.18.2",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.9.0",
    "mocha": "^2.4.5",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.17.1",
    "nyc": "^11.6.0",
    "sequelize-cli": "^4.0.0"
  },
  "scripts": {
    "migrate-undo": "sequelize db:migrate:undo:all --env test",
    "seed": "sequelize db:seed:all --env test",
    "migrate": "npm run migrate-undo && sequelize db:migrate --env test && npm run seed",
    "dev": "npm run pretest && babel-node server/index.js",
    "start": "nodemon server/index.js --exec babel-node",
    "dev-start": "nodemon server/index.js --exec \"npm run migrate && npm run test && babel-node\"",
    "build": "babel server -d dist",
    "serve": "node dist/index.js",
    "test": "nyc --reporter=html --reporter=text mocha server/tests/ --compilers js:babel-register",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "coveralls": "nyc --reporter=lcov --reporter=text-lcov npm test"
  }
}
